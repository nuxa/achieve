<h1>お問い合わせ</h1>
<%= form_for(@contact) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <br>
    <%= f.label :content %>
    <%= f.text_field :content %>
    <br>
    <%= f.submit %>
<% end %>

<%= link_to 'お問い合わせ一覧画面に戻る', contacts_path %>
